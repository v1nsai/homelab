{
  "ignition": {
    "version": "3.4.0"
  },
  "passwd": {
    "users": [
      {
        "groups": [
          "sudo",
          "docker"
        ],
        "name": "doctor_ew",
        "sshAuthorizedKeys": [
          "ssh-rsa "
        ]
      }
    ]
  },
  "storage": {
    "files": [
      {
        "overwrite": false,
        "path": "/etc/sudoers",
        "append": [
          {
            "compression": "",
            "source": "data:;base64,ZG9jdG9yX2V3ICAgICAgICBBTEw9KEFMTCkgICAgICAgTk9QQVNTV0Q6IEFMTAo="
          }
        ]
      }
    ],
    "links": [
      {
        "path": "/etc/localtime",
        "target": "/usr/share/zoneinfo/America/New_York"
      }
    ]
  },
  "systemd": {
    "units": [
      {
        "mask": true,
        "name": "locksmithd.service"
      },
      {
        "enabled": true,
        "name": "update-engine.service"
      }
    ]
  }
}
